{
  "dockerComposeFile": [
    "compose.yml"
  ],
  "service": "axum-shuttle",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "extensions": [
        "GitHub.copilot",
        "ms-azuretools.vscode-docker",
        "rust-lang.rust-analyzer",
        "tamasfe.even-better-toml",
        "serayuzgur.crates"
      ],
      "settings": {
        "files.watcherExclude": {
          "**/target/**": true
        }
      }
    }
  }
}